{
    "name": "amitdugar/archiveutil",
    "description": "Smart single-file compressor/decompressor for zstd, pigz/gzip, and zip with password-protected ZIP support",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/amitdugar/archiveutil",
    "keywords": [
        "archive",
        "compression",
        "zstd",
        "gzip",
        "pigz",
        "zip"
    ],
    "authors": [
        {
            "name": "Amit Dugar"
        }
    ],
    "support": {
        "issues": "https://github.com/amitdugar/archiveutil/issues",
        "source": "https://github.com/amitdugar/archiveutil"
    },
    "require": {
        "php": "^8.2",
        "symfony/process": "^7.3",
        "ext-zip": "*",
        "ext-zlib": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    },
    "autoload": {
        "psr-4": {
            "ArchiveUtil\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}